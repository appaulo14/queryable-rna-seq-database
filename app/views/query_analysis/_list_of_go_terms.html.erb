<% if go_ids_string.blank? %>
  No Go Terms found
<% else %>
  <ul>
    <% go_terms = go_terms_string.strip.split(';') %>
    <% go_ids = go_ids_string.strip.split(';') %>
    <% go_ids.length.times do |i| %>
      <li><%= link_to_amigo_web_page_for_term(go_terms[i],go_ids[i]) %></li>
    <% end %>
  </ul>
<% end %>